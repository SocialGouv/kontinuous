  "homepage": "https://socialgouv.github.io/kontinuous/",
  "license": "MIT",
  "main": "src/index",
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@sentry/node": "^7.30.0",
    "@types/node": "^18.0.0",
    "async-retry": "^1.3.3",
    "cli-color": "^2.0.3",
    "cli-highlight": "^2.1.11",
    "commander": "^10.0.0",
    "decompress": "^4.2.1",
    "dotenv": "^16.0.0",
    "dree": "^4.0.0",
    "enquirer": "^2.3.6",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.0",
    "helm-tree": "workspace:^",
    "launch-editor": "^2.4.0",
    "link-module-alias": "^1.2.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.defaults": "^4.2.0",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.get": "^4.4.2",
    "lodash.mergewith": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "lodash.set": "^4.3.2",
    "nctx": "^1.7.1",
    "nowtest": "^1.1.1",
    "qs": "^6.10.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "~common": "workspace:^"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.0.0",
    "jest-specific-snapshot": "^7.0.0"
  },
  "engines": {
    "node": "^16.17 || ^18"
  },
  "scripts": {
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest tests",
    "test:debug": "DEBUG=1 yarn test",
    "test:update-snapshots": "yarn test -u",
    "postinstall": "link-module-alias",
    "build": "yarn build:clean && yarn build:ncc && yarn build:symlink",
    "build:clean": "rm -rf ./dist",
    "build:ncc": "ncc build bin/kontinuous --source-map --v8-cache",
    "build:symlink": "[ -f ../../kontinuous-dist ] || ln -s packages/kontinuous/dist/index.js ../../kontinuous-dist"
  },
  "_moduleAliases": {
    "~": "src"
  },
  "bin": "./dist/index.js",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE",
    "tsconfig.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "La Fabrique Numérique des Ministères Sociaux",
    "email": "DSI-incubateur@sg.social.gouv.fr",
    "url": "https://www.fabrique.social.gouv.fr/"
  },
  "contributors": []
}
