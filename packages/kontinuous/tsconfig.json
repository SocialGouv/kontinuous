{
  "compilerOptions": {                        
    "lib": [
      "es2021"
    ],
    "module": "commonjs",
    "target": "es2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "allowJs": true,
    "noImplicitAny": false,
    "isolatedModules": false
  },
  "exclude":[
    "./node_modules"
  ]
}